<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>project-1</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>


	<servlet>
		<servlet-name>FrontController</servlet-name>
		<servlet-class>web.FrontController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>/loginEmployee</url-pattern>
		<url-pattern>/loginManager</url-pattern>
		<url-pattern>/logout</url-pattern>
		<url-pattern>/reimbursement</url-pattern>
		<url-pattern>/employeeInfo</url-pattern>
		<url-pattern>/updateInfo</url-pattern>
		<url-pattern>/employeeReimbursements</url-pattern>
		<url-pattern>/approveDeny</url-pattern>
		<url-pattern>/pendingReimbursements</url-pattern>
		<url-pattern>/resolvedReimbursements</url-pattern>
		<url-pattern>/allEmployees</url-pattern>
		<url-pattern>/singleEmployeeReimbursements</url-pattern>
	</servlet-mapping>

	<error-page>
		<error-code>404</error-code>
		<location>/error</location>
	</error-page>

	<!-- <servlet>
		<servlet-name>ErrorServlet</servlet-name>
		<servlet-class>com.revature.web.ErrorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ErrorServlet</servlet-name>
		<url-pattern>/error</url-pattern>
	</servlet-mapping> -->
</web-app>